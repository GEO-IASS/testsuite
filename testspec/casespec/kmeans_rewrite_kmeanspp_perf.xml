            <method>
                <name>km_pp_default_fndist_aggcentroid_maxiter_minfrac</name>
                <parameter>
                    <name>benchmark</name>
                    <value>TemplateExecutor</value>          
                </parameter> 
            </method>
        </methods>

       <test_suite>
            <name>km_pp_perf_norm1</name>
            <comments>Test performance of kmeans plusplus with norm1 distance function</comments>
                <!-- It indicate max execution times-->
                <execute_rate>1</execute_rate>
            <method>
                <name>km_pp_default_fndist_aggcentroid_maxiter_minfrac</name>
                <list_parameter>
                    <name>rel_source</name>
                    <value>madlibtestdata.km_us_census_1990</value>
                    <value>madlibtestdata.km_us_census_1990_rows_10_times</value>
                    <value>madlibtestdata.km_us_census_1990_rows_100_times</value>
                    <value>madlibtestdata.km_p53</value>
                    <value>madlibtestdata.km_p53_columns_10_times</value>
                    <value>madlibtestdata.km_p53_columns_100_times</value>
                    <value>madlibtestdata.km_tfidf3</value>
                    <value>madlibtestdata.km_tfidf3_rows_2_times</value>
                    <value>madlibtestdata.km_tfidf3_rows_4_times</value>
                    <value>madlibtestdata.km_tfidf3_columns_2_times</value>
                    <value>madlibtestdata.km_tfidf3_columns_4_times</value>
                </list_parameter>
                <parameters>
                    <parameter>
                        <name>expr_point</name>
                        <value>position</value>
                    </parameter>
                    <parameter>
                        <name>k</name>
                        <value>6</value>
                    </parameter>
                    <parameter>
                        <name>fn_dist_kmeans</name>
                        <value>madlib.dist_norm1</value>
                    </parameter>
                    <parameter>
                        <name>fn_dist_silhouette</name>
                        <value>madlib.dist_norm1</value>
                    </parameter>
                    <parameter>
                        <name>agg_centroid</name>
                        <value>madlib.avg</value>
                    </parameter>
                    <parameter>
                        <name>max_num_iterations</name>
                        <value>20</value>
                    </parameter>
                    <parameter>
                        <name>min_frac_reassigned</name>
                        <value>0.001</value>
                    </parameter>
                </parameters>
                <tear_down>
                </tear_down>
            </method>
       </test_suite>
        
