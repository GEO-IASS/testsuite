            <comments>It is to get baseline of logistic regression against R.</comments>
            <!-- It indicate max execution times-->
            <execute_rate>1</execute_rate>
            <method>
                <name>logregr</name>
                <list_parameter>
                    <name>source_table</name>
                    <value>madlibtestdata.log_breast_cancer_wisconsin</value><value>madlibtestdata.log_ticdata2000</value><value>madlibtestdata.log_wdbc</value><value>madlibtestdata.log_wpbc</value>
                </list_parameter>
                <list_parameter>
                    <name>optimizer</name>
                    <value>irls</value><value>newton</value><value>cg</value><value>igd</value>
                </list_parameter>
                <parameters>
                    <parameter>
                        <name>dependent_varname</name>
                        <value>y</value>
                    </parameter>
                    <parameter>
                        <name>independent_varname</name>
                        <value>x</value>
                    </parameter>
                    <parameter>
                        <name>max_iteration</name>
                        <value>2000</value>
                    </parameter>
                    <parameter>
                        <name>convergence_threshold</name>
                        <value>0.0</value>
                    </parameter>
