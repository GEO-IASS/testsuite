<?xml version='1.0' encoding='UTF-8'?>
<test_suites>
    <test_type>feature</test_type>
    <multi_test_suites>

    <algorithm>distribution_function</algorithm>
        <methods>
                <method>
                    <name>geometric_pmf</name>
                    <parameter>
                        <name>benchmark</name>
                        <value>TemplateExecutor</value>
                    </parameter>
                </method>
        </methods>
        <test_suite>
            <name>geometric_pmf_feature_test_x</name>
            <comments>geometric_pmf</comments>
            <execute_rate>1</execute_rate>
            <method>
                <name>geometric_pmf</name>
                <list_parameter>
                    <name>x</name>
                    <value>NULL</value>
                    <value>0</value>
                    <value>1</value>
                    <value>2</value>
                    <value>10</value>
                </list_parameter>
                <list_parameter>
                    <name>spp</name>
                    <value>0.5</value>
                </list_parameter>
                <tear_down></tear_down>
            </method>
        </test_suite>

        <test_suite>
            <name>geometric_pmf_feature_test_spp</name>
            <comments>geometric_pmf</comments>
            <execute_rate>1</execute_rate>
            <method>
                <name>geometric_pmf</name>
                <list_parameter>
                    <name>x</name>
                    <value>2</value>
                </list_parameter>
                <list_parameter>
                    <name>spp</name>
                    <value>0.000001</value>
                    <value>0.5</value>
                    <value>1</value>
                    <value>NULL</value>
                    <value>NaN</value>
                    <value>0</value>
                </list_parameter>
                <tear_down></tear_down>
            </method>
        </test_suite>

        <test_suite>
            <name>geometric_pmf_negative_case_x</name>
            <comments>geometric_pmf</comments>
            <execute_rate>1</execute_rate>
            <method>
                <name>geometric_pmf</name>
                <list_parameter>
                    <name>x</name>
                    <value>NaN</value>
                    <value>NINFINITY</value>
                    <value>+INFINITY</value>
                </list_parameter>
                <list_parameter>
                    <name>spp</name>
                    <value>0.5</value>
                </list_parameter>
                <tear_down></tear_down>
            </method>
        </test_suite>

        <test_suite>
            <name>geometric_pmf_negative_case_spp</name>
            <comments>geometric_pmf</comments>
            <execute_rate>1</execute_rate>
            <method>
                <name>geometric_pmf</name>
                <list_parameter>
                    <name>x</name>
                    <value>2</value>
                </list_parameter>
                <list_parameter>
                    <name>spp</name>
                    <value>-0.000001</value>
                    <value>1.000001</value>
                    <value>NINFINITY</value>
                    <value>+INFINITY</value>
                </list_parameter>
                <tear_down></tear_down>
            </method>
        </test_suite>

    </multi_test_suites>
</test_suites>
