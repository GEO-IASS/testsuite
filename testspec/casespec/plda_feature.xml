        <test_suite>
            <name>plda_basic_functionality_closed_set_test</name>
            <comments>It is to test basic functionality of plda with closed set test.</comments>
            <!-- It indicate max execution times-->
            <execute_rate>1</execute_rate>
            <method>
                <name>plda_run</name>
		<list_parameter>
		    <name>dataset</name>
		    <value>plda_run_20_newsgroups</value>
		    <value>plda_run_mini_20_newsgroups</value>
		    <value>plda_run_nsf_abs</value>
		    <value>plda_run_reuters21578</value>
		    <value>plda_run_reuterstranscribedsubset</value>
		</list_parameter>
            </method>
            <method>
                <name>plda_label_test_documents</name>
                <list_parameter>
                    <name>dataset</name>
                    <value>plda_label_20_newsgroups</value>
		    <value>plda_label_mini_20_newsgroups</value>
		    <value>plda_label_nsf_abs</value>
		    <value>plda_label_reuters21578</value>
		    <value>plda_label_reuterstranscribedsubset</value>
                </list_parameter>
                <tear_down>
                DROP TABLE IF EXISTS madlibtestresult.plda_basic_functionality_data;DROP TABLE IF EXISTS madlibtestresult.plda_basic_functionality_model;DROP TABLE IF EXISTS madlibtestresult.plda_basic_functionality_result;
                </tear_down>
            </method>
        </test_suite>

