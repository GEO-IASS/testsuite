<?xml version='1.0' encoding='UTF-8'?>
<test_suites>
    <test_type>feature</test_type>
    <multi_test_suites>

    <algorithm>distribution_function</algorithm>
        <methods>
                <method>
                    <name>chi_squared_cdf</name>
                    <parameter>
                        <name>benchmark</name>
                        <value>TemplateExecutor</value>
                    </parameter>
                </method>
        </methods>
        <test_suite>
            <name>chi_squared_cdf_feature_test_x</name>
            <comments>chi_squared_cdf</comments>
            <execute_rate>1</execute_rate>
            <method>
                <name>chi_squared_cdf</name>
                <list_parameter>
                    <name>x</name>
                    <value>NULL</value>
                    <value>NaN</value>
                    <value>NINFINITY</value>
                    <value>-3.5</value>
                    <value>-1</value>
                    <value>0</value>
                    <value>1</value>
                    <value>1.5</value>
                    <value>2</value>
                    <value>2.5</value>
                    <value>3</value>
                    <value>+INFINITY</value>
                </list_parameter>
                <list_parameter>
                    <name>df</name>
                    <value>2</value>
                    <value>4</value>
                </list_parameter>
                <tear_down></tear_down>
            </method>
        </test_suite>

        <test_suite>
            <name>chi_squared_cdf_feature_test_df</name>
            <comments>chi_squared_cdf</comments>
            <execute_rate>1</execute_rate>
            <method>
                <name>chi_squared_cdf</name>
                <list_parameter>
                    <name>x</name>
                    <value>2</value>
                </list_parameter>
                <list_parameter>
                    <name>df</name>
                    <value>0.00001</value>
                    <value>1</value>
                    <value>3</value>
                    <value>4</value>
                    <value>9</value>
                    <value>NULL</value>
                    <value>NaN</value>
                </list_parameter>
                <tear_down></tear_down>
            </method>
        </test_suite>

        <test_suite>
            <name>chi_squared_cdf_negative_case_df</name>
            <comments>chi_squared_cdf</comments>
            <execute_rate>1</execute_rate>
            <method>
                <name>chi_squared_cdf</name>
                <list_parameter>
                    <name>x</name>
                    <value>2</value>
                </list_parameter>
                <list_parameter>
                    <name>df</name>
                    <value>NINFINITY</value>
                    <value>-1.5</value>
                    <value>-1</value>
                    <value>0</value>
                    <value>+INFINITY</value>
                </list_parameter>
                <tear_down></tear_down>
            </method>
        </test_suite>

    </multi_test_suites>
</test_suites>
